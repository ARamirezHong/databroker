[run]
source = databroker
branch=True

[report]
omit =
    */python?.?/*
    */site-packages/nose/*
    *test*
    databroker/__init__.py
    databroker/tests/*
    databroker/testing/*
    # ignore _version.py and versioneer.py
    .*version.*
    *_version.py

exclude_lines =
    if __name__ == '__main__':
